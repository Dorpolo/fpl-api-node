{
  "name": "fpl-api-node",
  "version": "1.0.10",
  "description": "A simple node wrapper for the Fantasy Premier League (fantasy.premierleague.com) restful web api",
  "main": "dist/index.js",
  "typings": "dist/index",
  "files": [
    "dist/"
  ],
  "scripts": {
    "docs": "typedoc --out docs --name API --mode file --excludePrivate --theme markdown --markdownOutFile README.md --markdownSuppressIndexes",
    "lint": "tslint --project tsconfig.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "rm -rf dist && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tgreyuk/fpl-api-node.git"
  },
  "keywords": [
    "fantasy premier league",
    "fpl",
    "api"
  ],
  "author": "Thomas Grey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tgreyuk/fpl-api-node/issues"
  },
  "homepage": "https://github.com/tgreyuk/fpl-api-node#readme",
  "dependencies": {
    "@types/node-fetch": "1.6.7",
    "node-fetch": "1.7.1"
  },
  "devDependencies": {
    "tslint": "5.4.2",
    "typedoc": "0.7.1",
    "typedoc-plugin-markdown": "0.0.18",
    "typescript": "2.3.4"
  }
}
