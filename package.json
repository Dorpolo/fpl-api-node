{
  "name": "fpl-api-node",
  "version": "2.2.4",
  "description": "A node wrapper for the Fantasy Premier League (fantasy.premierleague.com) restful web api",
  "main": "dist/index.js",
  "typings": "dist/index",
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "report-coverage": "codecov",
    "test": "nyc mocha -r ts-node/register test/test.spec.ts --timeout 5000 --slow 0",
    "prepare": "rm -rf dist && tsc",
    "prepublishOnly": "npm run prepare && npm run lint && npm run test",
    "task:wiki": "ts-node tasks/wiki/src/index.ts",
    "mock:data": "ts-node tasks/mock-generator/generate-data.ts",
    "mock:methods": "ts-node tasks/mock-generator/generate-methods.ts",
    "docs": "typedoc --out ./docs-html ./src --excludeExternals --exclude \"**/*+(data.*|.enum).ts\" --excludePrivate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tgreyjs/fpl-api-node.git"
  },
  "keywords": [
    "fantasy premier league",
    "fpl",
    "api"
  ],
  "author": "Thomas Grey",
  "license": "MIT",
  "engines": {
    "node": ">=7"
  },
  "bugs": {
    "url": "https://github.com/tgreyjs/fpl-api-node/issues"
  },
  "dependencies": {
    "@types/lodash": "^4.14.106",
    "@types/node-cache": "^4.1.1",
    "async": "^2.6.0",
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "lodash": "^4.17.5",
    "node-cache": "^4.2.0"
  },
  "devDependencies": {
    "@types/axios-mock-adapter": "^1.10.0",
    "@types/chai": "^4.1.2",
    "@types/handlebars": "^4.0.36",
    "@types/jsonfile": "^4.0.1",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.1",
    "@types/sinon": "^4.3.0",
    "axios-mock-adapter": "^1.14.1",
    "chai": "^4.1.2",
    "handlebars": "^4.0.11",
    "jsonfile": "^4.0.0",
    "mocha": "^5.0.5",
    "nyc": "^11.6.0",
    "rimraf": "^2.6.2",
    "sinon": "^4.4.9",
    "ts-node": "^5.0.1",
    "ts-simple-ast": "^10.0.1",
    "tslint": "^5.9.1",
    "typedoc": "^0.11.1",
    "typedoc-plugin-external-module-name": "^1.1.1",
    "typescript": "^2.8.1"
  },
  "greenkeeper": {
    "ignore": [
      "ts-simple-ast"
    ]
  }
}
