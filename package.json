{
  "name": "fpl-api-node",
  "version": "2.2.4",
  "description": "A node wrapper for the Fantasy Premier League (fantasy.premierleague.com) restful web api",
  "main": "dist/index.js",
  "typings": "dist/index",
  "files": [
    "dist/",
    "schemas/"
  ],
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "test": "nyc mocha -r ts-node/register test/test.ts --timeout 5000 --slow 0",
    "prepare": "rm -rf dist && tsc",
    "prepublishOnly": "npm run prepare && npm run lint && npm run test",
    "mock:data": "rm -rf test/fixtures/data/* && ts-node tasks/mock-generator/generate-data.ts",
    "mock:methods": "rm -rf test/fixtures/methods/* && ts-node tasks/mock-generator/generate-methods.ts",
    "wiki": "rm -rf tasks/wiki/out/* && ts-node tasks/wiki/src/index.ts",
    "graphql:server":"onchange 'src/**/*.ts' 'tasks/**/*.ts' 'graphql/schema.graphql' -i -- ts-node src/graphql.ts",
    "graphql:generate": "ts-node tasks/graphql-generator/index.ts",
    "schema-json": "apollo-codegen introspect-schema schemas/schema.graphql --output schemas/schema.json",
    "docs": "graphdoc -s ./graphql/schema.graphql -o ./docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tgreyjs/fpl-api-node.git"
  },
  "keywords": [
    "fantasy premier league",
    "fpl",
    "api"
  ],
  "author": "Thomas Grey",
  "license": "MIT",
  "engines": {
    "node": ">=7"
  },
  "bugs": {
    "url": "https://github.com/tgreyjs/fpl-api-node/issues"
  },
  "dependencies": {
    "@types/async": "^2.0.49",
    "@types/graphql": "^0.13.1",
    "@types/lodash": "^4.14.107",
    "@types/node-cache": "^4.1.1",
    "apollo-server-express": "^1.3.6",
    "async": "^2.6.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-import": "^0.5.2",
    "graphql-resolvers": "^0.2.2",
    "graphql-tools": "^3.0.1",
    "lodash": "^4.17.5",
    "node-cache": "^4.2.0",
    "router": "^1.3.2"
  },
  "devDependencies": {
    "@types/axios-mock-adapter": "^1.10.0",
    "@types/chai": "^4.1.2",
    "@types/fs-extra": "^5.0.2",
    "@types/handlebars": "^4.0.37",
    "@types/jsonfile": "^4.0.1",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.5",
    "@types/sinon": "^4.3.1",
    "apollo-codegen": "^0.19.1",
    "axios-mock-adapter": "^1.15.0",
    "chai": "^4.1.2",
    "fs-extra": "^6.0.1",
    "handlebars": "^4.0.11",
    "jsonfile": "^4.0.0",
    "mocha": "^5.1.0",
    "nyc": "^11.6.0",
    "onchange": "^4.0.0",
    "sinon": "^4.5.0",
    "ts-node": "^6.0.3",
    "ts-node-dev": "^1.0.0-pre.23",
    "ts-simple-ast": "^10.3.2",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  }
}
