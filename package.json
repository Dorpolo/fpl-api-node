{
  "name": "fpl-api-node",
  "version": "2.2.1",
  "description": "A node wrapper for the Fantasy Premier League (fantasy.premierleague.com) restful web api",
  "main": "dist/index.js",
  "typings": "dist/index",
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "report-coverage": "codecov",
    "test": "nyc mocha -r ts-node/register test/specs.ts --timeout 5000 --slow 0",
    "prepare": "rm -rf dist && tsc -p src/tsconfig.json",
    "prepublishOnly": "npm run prepare && npm run lint && npm run test",
    "wiki": "ts-node tasks/wiki/src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tgreyuk/fpl-api-node.git"
  },
  "keywords": [
    "fantasy premier league",
    "fpl",
    "api"
  ],
  "author": "Thomas Grey",
  "license": "MIT",
  "engines": {
    "node": ">=7"
  },
  "bugs": {
    "url": "https://github.com/tgreyjs/fpl-api-node/issues"
  },
  "homepage": "https://github.com/tgreyjs/fpl-api-node#readme",
  "dependencies": {
    "@types/node-cache": "^4.1.1",
    "axios": "^0.17.1",
    "node-cache": "^4.1.1",
    "rxjs": "^5.5.6"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/handlebars": "^4.0.36",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.6",
    "@types/sinon": "^4.1.3",
    "chai": "^4.1.2",
    "handlebars": "^4.0.11",
    "mocha": "^5.0.1",
    "nyc": "^11.4.1",
    "sinon": "^4.3.0",
    "ts-node": "^4.1.0",
    "ts-simple-ast": "^7.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  },
  "greenkeeper": {
    "ignore": [
      "ts-simple-ast"
    ]
  }
}
