{
  "name": "fpl-api-node",
  "version": "2.2.4",
  "description": "A node wrapper for the Fantasy Premier League (fantasy.premierleague.com) restful web api",
  "main": "dist/index.js",
  "typings": "dist/index",
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "test": "nyc mocha -r ts-node/register test/test.ts --timeout 5000 --slow 0",
    "prepare": "rm -rf dist && tsc",
    "prepublishOnly": "npm run prepare && npm run lint && npm run test",
    "mock:data": "rm -rf test/fixtures/data/* && ts-node tasks/mock-generator/generate-data.ts",
    "mock:methods": "rm -rf test/fixtures/methods/* && ts-node tasks/mock-generator/generate-methods.ts",
    "wiki": "rm -rf tasks/wiki/out/* && ts-node tasks/wiki/src/index.ts",
    "x":"ts-node src/routes/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tgreyjs/fpl-api-node.git"
  },
  "keywords": [
    "fantasy premier league",
    "fpl",
    "api"
  ],
  "author": "Thomas Grey",
  "license": "MIT",
  "engines": {
    "node": ">=7"
  },
  "bugs": {
    "url": "https://github.com/tgreyjs/fpl-api-node/issues"
  },
  "dependencies": {
    "@types/async": "^2.0.49",
    "@types/lodash": "^4.14.107",
    "@types/node-cache": "^4.1.1",
    "async": "^2.6.0",
    "axios": "^0.18.0",
    "lodash": "^4.17.5",
    "node-cache": "^4.2.0",
    "router": "^1.3.2"
  },
  "devDependencies": {
    "@types/axios-mock-adapter": "^1.10.0",
    "@types/chai": "^4.1.2",
    "@types/express": "^4.11.1",
    "@types/handlebars": "^4.0.37",
    "@types/jsonfile": "^4.0.1",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.5",
    "@types/sinon": "^4.3.1",
    "axios-mock-adapter": "^1.15.0",
    "chai": "^4.1.2",
    "express": "^4.16.3",
    "handlebars": "^4.0.11",
    "jsonfile": "^4.0.0",
    "mocha": "^5.1.0",
    "nyc": "^11.6.0",
    "sinon": "^4.5.0",
    "ts-node": "^6.0.3",
    "ts-simple-ast": "^10.3.2",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  }
}
