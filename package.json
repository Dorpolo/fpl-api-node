{
  "name": "fpl-api-node",
  "version": "2.0.1",
  "description": "A node wrapper for the Fantasy Premier League (fantasy.premierleague.com) restful web api",
  "main": "dist/index.js",
  "typings": "dist/index",
  "files": [
    "dist/"
  ],
  "scripts": {
    "generateWiki": "ts-node tasks/wiki/src/index.ts",
    "lint": "tslint --project tsconfig.json",
    "report-coverage": "codecov",
    "test": "nyc mocha -r ts-node/register test/specs.ts --timeout 5000 --slow 0",
    "prepare": "rm -rf dist && tsc -p src/tsconfig.json && npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tgreyuk/fpl-api-node.git"
  },
  "keywords": [
    "fantasy premier league",
    "fpl",
    "api"
  ],
  "author": "Thomas Grey",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "bugs": {
    "url": "https://github.com/tgreyuk/fpl-api-node/issues"
  },
  "homepage": "https://github.com/tgreyuk/fpl-api-node#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "node-cache": "^4.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.0.3",
    "@types/handlebars": "^4.0.35",
    "@types/lodash": "^4.14.73",
    "@types/mocha": "^2.2.41",
    "@types/node-cache": "^4.1.0",
    "@types/sinon": "^2.3.3",
    "chai": "^4.1.1",
    "codecov": "^2.3.0",
    "handlebars": "^4.0.10",
    "lodash": "^4.17.4",
    "mocha": "^3.5.0",
    "nyc": "^11.1.0",
    "sinon": "^3.2.1",
    "ts-node": "^3.3.0",
    "ts-simple-ast": "^0.52.0",
    "tslint": "^5.6.0",
    "typescript": "^2.4.2"
  }
}
