{
  "name": "fpl-api-node",
  "version": "2.0.0-alpha.0",
  "description": "A simple node wrapper for the Fantasy Premier League (fantasy.premierleague.com) restful web api",
  "main": "dist/index.js",
  "typings": "dist/index",
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "test": "mocha -r ts-node/register test/specs.ts --timeout 15000 --slow 0",
    "prepare": "rm -rf dist && tsc -p src/tsconfig.json",
    "prepublish": "npm run prepare && npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tgreyuk/fpl-api-node.git"
  },
  "keywords": [
    "fantasy premier league",
    "fpl",
    "api"
  ],
  "author": "Thomas Grey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tgreyuk/fpl-api-node/issues"
  },
  "homepage": "https://github.com/tgreyuk/fpl-api-node#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "lodash": "^4.17.4",
    "node-cache": "^4.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/lodash": "^4.14.71",
    "@types/mocha": "^2.2.41",
    "@types/node-cache": "^4.1.0",
    "async": "^2.5.0",
    "chai": "^4.1.0",
    "mocha": "^3.4.2",
    "ts-node": "^3.3.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.2"
  }
}
