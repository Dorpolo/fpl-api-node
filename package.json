{
  "name": "fpl-api-node",
  "version": "2.0.3",
  "description": "A node wrapper for the Fantasy Premier League (fantasy.premierleague.com) restful web api",
  "main": "dist/index.js",
  "typings": "dist/index",
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "report-coverage": "codecov",
    "test": "nyc mocha -r ts-node/register test/specs.ts --timeout 5000 --slow 0",
    "prepare": "rm -rf dist && tsc -p src/tsconfig.json",
    "prepublishOnly": "npm run prepare && npm run lint && npm run test",
    "wiki": "ts-node tasks/wiki/src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tgreyuk/fpl-api-node.git"
  },
  "keywords": [
    "fantasy premier league",
    "fpl",
    "api"
  ],
  "author": "Thomas Grey",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "bugs": {
    "url": "https://github.com/tgreyuk/fpl-api-node/issues"
  },
  "homepage": "https://github.com/tgreyuk/fpl-api-node#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "memory-cache": "^0.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/handlebars": "^4.0.36",
    "@types/lodash": "^4.14.74",
    "@types/memory-cache": "0.0.31",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.28",
    "@types/sinon": "^2.3.3",
    "chai": "^4.1.2",
    "handlebars": "^4.0.10",
    "lodash": "^4.17.4",
    "mocha": "^3.5.1",
    "nyc": "^11.2.1",
    "sinon": "^3.2.1",
    "ts-node": "^3.3.0",
    "ts-simple-ast": "^0.61.2",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2"
  },
  "greenkeeper": {
    "ignore": [
      "ts-simple-ast"
    ]
  }
}
