{
  "extends": [
    "tslint:latest"
  ],
  "rules": {
    "interface-name": [
      false
    ],
    "quotemark": [
      true,
      "single"
    ],
    "max-classes-per-file": [
      false
    ],
    "no-console": [
      false
    ],
    "no-submodule-imports": [
      false
    ],
    "no-implicit-dependencies": [
      false
    ],
    "object-literal-sort-keys": [
      false
    ],
    "no-var-requires": [
      false
    ],
    "max-line-length": [
      true,
      140
    ],
    "no-angle-bracket-type-assertion": [
      false
    ]
  }
}
